<configuration description="Runs charge_arm_test.">
    <target_preparer class="com.android.tradefed.targetprep.PushFilePreparer">
        <option name="push-file" key="charge_arm_test" value="/data/local/tmp/charge_arm_test" />
    </target_preparer>
    <target_preparer class="com.android.tradefed.targetprep.RootTargetPreparer"/>

    <option name="test-tag" value="charge_arm_test" />

    <test class="com.android.tradefed.testtype.GTest" >
        <option name="native-test-device-path" value="/data/local/tmp" />
        <option name="module-name" value="charge_arm_test" />
    </test>

    <result_reporter class="com.android.tradefed.result.ConsoleResultReporter" />
    <result_reporter class="com.android.tradefed.result.XmlResultReporter" />
</configuration>
